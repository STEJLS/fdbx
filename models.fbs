// FlatBuffers Schema
// http://google.github.io/flatbuffers/

namespace models;

table File {
	Path:string;
	MTime:uint64;
	Data:[uint8];
}

table TestRecord {
	Name:string;
	Number:uint64;
	Float:float64;
	Logic:bool;
	Data:[uint8];
	Strings:[string];
}

table Transaction {
    TxID:uint64;
    Start:uint64;
    Committed:byte=2;
}