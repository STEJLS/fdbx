// FlatBuffers Schema
// http://google.github.io/flatbuffers/

namespace models;

table File {
	Path:string;
	MTime:uint64;
	Data:[uint8];
}

table TestRecord {
	Name:string;
	Number:uint64;
	Float:float64;
	Logic:bool;
	Data:[uint8];
	Strings:[string];
}

table Transaction {
    TxID:uint64;
    Start:uint64;
    Status:uint8=3;
}

table Row {
    XMin:uint64;
    XMax:uint64=0;
    CMin:uint32;
    CMax:uint32=0;
    Hash:uint64=0;
    Blob:bool=false;
    GZip:bool=false;
    Data:[uint8];
}