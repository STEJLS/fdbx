// FlatBuffers Schema
// http://google.github.io/flatbuffers/

namespace models;

table Transaction {
    TxID:uint64;
    Start:uint64;
    Status:uint8=3;
}

struct RowState {
    XMin:uint64;
    XMax:uint64;
    CMin:uint32;
    CMax:uint32;
}

table Row {
    State:RowState (required);
    Data:[uint8];
}

table Value {
    Blob:bool;
    GZip:bool;
    Hash:uint64;
    Data:[uint8];
}